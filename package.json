{
  "name": "alphabet-list-svelte",
  "version": "1.0.1",
  "description": "A reusable Svelte component for displaying a list of items with alphabet filtering.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "svelte": "src/lib/AlphabetList.svelte",
  "exports": {
    ".": {
      "svelte": "./src/AlphabetList.svelte",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.7",
    "vite": "^5.0.3"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "alphabet",
    "filter",
    "component"
  ],
  "author": "Ayush Jhunjhunwala",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
